<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".development.DevelopmentMainActivity">

    <Button
        android:id="@+id/dev_mvc_bt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="MVC测试"
        android:textSize="20dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="30dp"
        />

    <Button
        android:id="@+id/dev_mvp_bt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="MVP测试"
        android:textSize="20dp"
        app:layout_constraintLeft_toLeftOf="@id/dev_mvc_bt"
        app:layout_constraintTop_toBottomOf="@+id/dev_mvc_bt"
        android:layout_marginTop="30dp"
        />

    <Button
        android:id="@+id/dev_mvvm_bt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="MVVM测试"
        android:textSize="20dp"
        app:layout_constraintLeft_toLeftOf="@id/dev_mvp_bt"
        app:layout_constraintTop_toBottomOf="@+id/dev_mvp_bt"
        android:layout_marginTop="30dp"
        />

    <Button
        android:id="@+id/dev_mvi_bt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="MVI测试"
        android:textSize="20dp"
        app:layout_constraintLeft_toLeftOf="@id/dev_mvvm_bt"
        app:layout_constraintTop_toBottomOf="@+id/dev_mvvm_bt"
        android:layout_marginTop="30dp"
        />


</androidx.constraintlayout.widget.ConstraintLayout>